<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="co.edu.uptc.controllerFx.ListMoviesController">
    <children>
        <TableView fx:id="tableView" prefHeight="400.0" prefWidth="469.0">
            <columns>
                <TableColumn fx:id="nameColumn" prefWidth="117.0" text="Name" />
                <TableColumn fx:id="directorColumn" prefWidth="110.0" text="Director" />
                <TableColumn fx:id="yearColumn" prefWidth="113.0" text="Year" />
                <TableColumn fx:id="generColumn" prefWidth="96.0" text="Gener" />
            </columns>
        </TableView>
        <Button mnemonicParsing="false" onAction="#showFormCreateMovie" text="Add Movie" />
    </children>
</VBox>